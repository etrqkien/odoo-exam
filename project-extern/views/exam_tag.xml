<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="tree_exam_tag" model="ir.ui.view">
            <field name="name">exam_tag</field>
            <field name="model">exam.tag</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="form_exam_tag" model="ir.ui.view">
            <field name="name">exam_tag</field>
            <field name="model">exam.tag</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="exam_tag_act" model="ir.actions.act_window">
            <field name="name">exam_tag</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">exam.tag</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>