<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="exam_group" model="ir.module.category">
            <field name="name">Nhom quyen trong thu vien</field>
            <field name="sequence" eval="1"/>
        </record>
        <record id="exam_manager" model="res.groups">
            <field name="name">Admin</field>
            <field name="category_id" ref="exam_group"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="exam_employee" model="res.groups">
            <field name="name">Employee</field>
            <field name="category_id" ref="exam_group"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
    </data>
</odoo>